package co.jp.netwisdom.dto;

 

public class HobbyDto {
	
	//用户名
	private String username;
	//爱好
	private String hobby;




    







	public HobbyDto(String hobby) {
		super();
		this.hobby = hobby;
	}











	public String getHobby() {
		return hobby;
	}











	public void setHobby(String hobby) {
		this.hobby = hobby;
	}











	


}
	