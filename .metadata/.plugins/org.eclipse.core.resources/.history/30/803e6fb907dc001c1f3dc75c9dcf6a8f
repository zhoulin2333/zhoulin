package cn.key.mapping;

import java.sql.ResultSet;
import java.sql.SQLException;

import cn.key.dbManager.IResultSetMapping;
import cn.key.entity.BookInfo;
import co.jp.zhoulin.dto.ZLUserHobby;

public class ZlUserHobbyMapping implements IResultSetMapping{

	@Override
	public ZLUserHobby mapping(ResultSet rs) throws SQLException {
		int i = 1;
		ZLUserHobby zLUserHobby = new ZLUserHobby(rs.getString(i++), 
												  rs.getString(i++), 
												  rs.getString(i++), 
												  rs.getString(i++),
												  rs.getString(i++), 
												  rs.getString(i++));
		return zLUserHobby;
	}
	
}
